      "name": "Login USER",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "// Validate HTTP status",
              "pm.test(\"Status code is 200\", function () {",
              "    pm.response.to.have.status(200);",
              "});",
              "",
              "// Validate content type is JSON",
              "pm.test(\"Response is JSON\", function () {",
              "    pm.response.to.be.json;",
              "});",
              "",
              "// Parse JSON safely",
              "let jsonData = null;",
              "try {",
              "    jsonData = pm.response.json();",
              "} catch (e) {",
              "    // Leave jsonData as null so subsequent tests show clear failures",
              "}",
              "",
              "// Basic shape checks",
              "pm.test(\"Response body is an object\", function () {",
              "    pm.expect(jsonData, \"Response body is not valid JSON\").to.be.an(\"object\");",
              "});",
              "",
              "pm.test(\"Response has keys: status and token\", function () {",
              "    pm.expect(jsonData).to.have.property(\"status\");",
              "    pm.expect(jsonData).to.have.property(\"token\");",
              "});",
              "",
              "// Expected values",
              "pm.test(\"Status equals 'success'\", function () {",
              "    pm.expect(jsonData && jsonData.status, \"Missing or invalid status\").to.eql(\"success\");",
              "});",
              "",
              "// Token checks",
              "pm.test(\"Token is a non-empty string\", function () {",
              "    pm.expect(jsonData && jsonData.token, \"Missing token\").to.be.a(\"string\").and.not.empty;",
              "});",
              "",
              "pm.test(\"Token resembles a JWT (three segments)\", function () {",
              "    const token = jsonData && jsonData.token;",
              "    pm.expect(token && token.split(\".\").length, \"Token does not look like a JWT\").to.eql(3);",
              "});",
              "",
              "// Persist token to environment (preserves your original behavior)",
              "if (jsonData && jsonData.token) {",
              "    pm.environment.set(\"userToken\", jsonData.token);",
              "}",
              "",
              "// Verify environment variable is set",
              "pm.test(\"Token is set in environment\", function () {",
              "    pm.expect(pm.environment.get(\"userToken\")).to.not.be.null;",
              "});"
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
